            rdfs:range xsd:dateTime .


#################################################################
#    Classes
#################################################################

###  http://www.marioverhaeg.nl/rp#Addressed_Challenge
:Addressed_Challenge rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :Challenge
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :addressed_by ;
                                                                  owl:someValuesFrom :Requirement
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Challenge .


###  http://www.marioverhaeg.nl/rp#Addressed_Insight
:Addressed_Insight rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty :addressed_by ;
                                         owl:someValuesFrom :Requirement
                                       ] ;
                   rdfs:subClassOf :Insight .


###  http://www.marioverhaeg.nl/rp#Addressed_Opportunity
:Addressed_Opportunity rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :Opportunity
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :addressed_by ;
                                                                    owl:someValuesFrom :Requirement
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :Opportunity .


###  http://www.marioverhaeg.nl/rp#Challenge
:Challenge rdf:type owl:Class ;
           rdfs:subClassOf :Insight ;
           owl:disjointWith :Opportunity .


###  http://www.marioverhaeg.nl/rp#Challenge_Frustration
:Challenge_Frustration rdf:type owl:Class ;
                       rdfs:subClassOf :Information .


###  http://www.marioverhaeg.nl/rp#Complete_Information
:Complete_Information rdf:type owl:Class ;
                      owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :data_argumentation ;
                                                                   owl:someValuesFrom xsd:string
                                                                 ]
                                                                 [ rdf:type owl:Restriction ;
                                                                   owl:onProperty :data_value ;
                                                                   owl:someValuesFrom xsd:int
                                                                 ]
                                                               ) ;
                                            rdf:type owl:Class
                                          ] ;
                      rdfs:subClassOf :Information .


###  http://www.marioverhaeg.nl/rp#Contextual_Circumstance
:Contextual_Circumstance rdf:type owl:Class ;
                         rdfs:subClassOf :Evidence ;
                         owl:disjointWith :Evaluated_External_Evidence ,
                                          :Stakeholder_Evidence .


###  http://www.marioverhaeg.nl/rp#Evaluated_External_Evidence
:Evaluated_External_Evidence rdf:type owl:Class ;
                             rdfs:subClassOf :Evidence ;
                             owl:disjointWith :Stakeholder_Evidence .


