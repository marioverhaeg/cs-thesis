		sh:message "Completeness: expand Vision with current_state."; ].

rp:InformationShape 
	a sh:NodeShape;
	sh:targetClass rp:Information; 
	sh:property [
		sh:or (
			[sh:path rp:based_on_evidence; sh:minCount 1;]
			[sh:path rp:based_on_information; sh:minCount 1;]
		);
		sh:severity sh:Violation; 
		sh:minCount 1; 
		sh:message "Reproducibility: enter an information or evidence source for this information."; ];
	sh:property [
		sh:path rp:data_value;
		sh:severity sh:Violation; 
		sh:minCount 1; 
		sh:message "Completeness: expand Information with a value."; ];
	sh:property [
		sh:path rp:data_argumentation;
		sh:severity sh:Violation; 
		sh:minCount 1; 
		sh:message "Completeness: expand Information with argumentation."; ].
		
