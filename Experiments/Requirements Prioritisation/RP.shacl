		sh:severity sh:Violation; 
		sh:minCount 1; 
		sh:message "Completeness: expand Requirement with decrease_cost."; ];
	sh:property [
		sh:path rp:has_information_requirement_decrease_risk;
		sh:severity sh:Violation; 
		sh:minCount 1; 
	sh:message "Completeness: expand Requirement with decrease_risk."; ];
		sh:property [
		sh:path rp:addresses; 
		sh:severity sh:Violation; 
